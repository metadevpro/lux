  "author": {
    "name": "Metadev S.L.",
    "url": "https://metadev.pro"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:metadevpro/lux.git"
  },
  "bugs": {
    "url": "https://github.com/metadevpro/lux/issues"
  },
